[Unit]
Description = voctomix {{ item }}-to-framebuffer playout
After = voctocore.service
Requires = voctocore.service

[Service]
Type = simple
ExecStart = /opt/voctomix/scripts/{{ item }}-to-framebuffer-playout.sh
Restart = always
RestartSec = 1s
StartLimitInterval = 0

[Install]
WantedBy = voctocore.service
