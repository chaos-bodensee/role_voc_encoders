[Unit]
Description = voctomix {{ item.1.name }}-source
After = voctocore.service
Requires = voctocore.service

[Service]
Type = simple
ExecStart = /opt/voctomix/scripts/{{ item.1.name }}-source.sh
Restart = always
RestartSec = 1s
StartLimitInterval = 0

[Install]
WantedBy = voctocore.service
